  "publisher": "jerrygoyal",
  "author": {
    "name": "Gourav Goyal"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "license": "SEE license IN LICENSE file",
  "icon": "images/menubar.png",
  "galleryBanner": {
    "color": "#752d00",
    "theme": "dark"
